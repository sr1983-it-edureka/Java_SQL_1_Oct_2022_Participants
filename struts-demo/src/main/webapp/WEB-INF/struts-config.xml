<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
"http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">


<struts-config>

	<form-beans>
		<form-bean name="welcomeModel" type="com.edureka.javasql1.strutsdemo.WelcomeModel"/>
	</form-beans>
	
	<action-mappings>
	
		<action path="/struts-welcome" type="com.edureka.javasql1.strutsdemo.WelcomeAction"
			name="welcomeModel">
			<forward name="success" path="/welcome.jsp" />
		</action>
	</action-mappings>

</struts-config>
